  keywords = {accessibility,assistive technologies,blind users,qualitative study,social networking services},
  mendeley-groups = {Referencias},
  pages = {1584--1595},
  title = {{Blind People and Social Networking Services:  Challenges, Strategies, and Design Opportunities}},
  year = {2016}
}

